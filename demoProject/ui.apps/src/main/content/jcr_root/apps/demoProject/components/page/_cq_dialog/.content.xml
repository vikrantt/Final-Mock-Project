<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured" jcr:title="My Page Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/tabs"
                          maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <tab1 jcr:primaryType="nt:unstructured"
                                  jcr:title="Custom Properties"
                                  sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <presets jcr:primaryType="nt:unstructured"
                                             sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                             fieldLabel="Display Header Links" name="./presets">
                                        <items jcr:primaryType="nt:unstructured">
                                            <option1 jcr:primaryType="nt:unstructured"
                                                     text="Home" value="Home" />
                                            <option2 jcr:primaryType="nt:unstructured"
                                                     text="Published Blogs" value="Published Blogs" />
                                            <option3 jcr:primaryType="nt:unstructured"
                                                     text="Both" value="Both" />
                                        </items>
                                    </presets>
                                    <tab2 jcr:primaryType="nt:unstructured"
                                          jcr:title="Blogs List"
                                          sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <noOfBlogs jcr:primaryType="nt:unstructured"
                                                       sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                       fieldLabel="Number Of Blogs" name="./noOfBlogs" />
                                        </items>
                                    </tab2>
                                    <tab3 jcr:primaryType="nt:unstructured"
                                          sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                          fieldLabel="Display Footer Links" name="./tab3">
                                        <items jcr:primaryType="nt:unstructured">
                                            <option1 jcr:primaryType="nt:unstructured"
                                                     text="BackToHome" value="BackToHome" />
                                            <option2 jcr:primaryType="nt:unstructured"
                                                     text="Blogs" value="Blogs" />
                                            <option3 jcr:primaryType="nt:unstructured"
                                                     text="All" value="All" />
                                        </items>
                                    </tab3>
                                </items>
                            </tab1>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>