<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Enter Header Elements" sling:resourceType="cq/gui/components/authoring/dialog" helpPath="https://www.adobe.com/go/aem_cmp_image_v2">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <columns jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                                    maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <tab1
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Java"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">

                                            <teaserImage1
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link to"
                                                    name="./teaserimage1"
                                                    rootPath="/content"
                                                    suffix=".html"/>
                                            <heading1
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Heading Text"
                                                    name="./heading1"/>
                                            <headLink1
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link for Heading:"
                                                    href="www.google.com"
                                                    name="./headLink1"
                                                    target="_blank"/>
                                            <subheading1
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                    fieldLabel="Subheading Text"
                                                    name="./subheading1"/>
                                            <contLink1
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link for Continue:"
                                                    href="www.google.com"
                                                    name="./contLink1"
                                                    target="_blank"/>

                                        </items>
                                    </tab1>

                                    <tab2
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="AEM"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">

                                            <teaserImage2
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link to"
                                                    name="./teaserimage2"
                                                    rootPath="/content"
                                                    suffix=".html"/>
                                            <heading2
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Text"
                                                    name="./heading2"/>
                                            <headLink2
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link for Heading:"
                                                    href="www.google.com"
                                                    name="./headLink2"
                                                    target="_blank"/>
                                            <subheading2
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                    fieldLabel="Text"
                                                    name="./subheading2"/>
                                            <contLink2
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link for Continue:"
                                                    href="www.google.com"
                                                    name="./contLink2"
                                                    target="_blank"/>


                                        </items>
                                    </tab2>

                                    <tab3
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="JS"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">

                                            <teaserImage3
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link to"
                                                    name="./teaserimage3"
                                                    rootPath="/content"
                                                    suffix=".html"/>
                                            <heading3
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Text"
                                                    name="./heading3"/>
                                            <headLink2
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link for Heading:"
                                                    href="www.google.com"
                                                    name="./headLink3"
                                                    target="_blank"/>
                                            <subheading3
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                    fieldLabel="Text"
                                                    name="./subheading3"/>
                                            <contLink3
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link for Continue:"
                                                    href="www.google.com"
                                                    name="./contLink3"
                                                    target="_blank"/>


                                        </items>
                                    </tab3>
                                </items>
                            </tabs>





                        </items>
                    </column>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>